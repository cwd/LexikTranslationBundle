<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="cwd.vtiger.client.class">Cwd\VtigerBundle\Service\VtigerClient</parameter>
    </parameters>

    <services>

        <service id="cwd.vtiger.client" class="%cwd.vtiger.client.class%">
            <argument>%cwd.vtiger.config%</argument>
        </service>
    </services>
</container>
